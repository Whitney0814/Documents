CREATE TABLE EXAMPLE(
    A NUMBER(10),
    B NUMBER(10),
    C NUMBER(10)
);

DROP TABLE EXAMPLE;

CREATE TABLE EXAMPLE(
    id NUMBER(10) NOT NULL,
    A NUMBER(10),
    B NUMBER(10),
    C NUMBER(10)
);

DROP TABLE EXAMPLE;

CREATE TABLE EXAMPLE(
    id NUMBER(10) NOT NULL,
    A NUMBER(10),
    B NUMBER(10),
    C NUMBER(10),
    CONSTRAINT EXAMPLE_PK PRIMARY KEY (id)
);
